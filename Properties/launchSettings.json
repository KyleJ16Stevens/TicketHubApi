{
  "$schema": "https://json.schemastore.org/launchsettings.json",
  "profiles": {
    "http": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "launchBrowser": false,
      "applicationUrl": "http://localhost:5231",
        "environmentVariables": {
            "ASPNETCORE_ENVIRONMENT": "Development",
            "AZURE_STORAGE_CONNECTION_STRING": "YOUR_DEV_SECRET"
        }
    },
    "https": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "launchBrowser": false,
      "applicationUrl": "https://localhost:7140;http://localhost:5231",
        "environmentVariables": {
            "ASPNETCORE_ENVIRONMENT": "Development",
            "AZURE_STORAGE_CONNECTION_STRING": "YOUR_DEV_SECRET"
        }
    }
  }
}
